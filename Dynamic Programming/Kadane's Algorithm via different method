//...........KADANE'S ALGORITHM...........................//

#include <bits/stdc++.h>
using namespace std;

void *max_sum(int arr[],int n,int count){
    int currentsum=0,maxsum=0;
    for(int i=0;i<n;i++){
        currentsum+=arr[i];
        if(currentsum<0)
        currentsum=0;
        maxsum=max(currentsum,maxsum);
        if(arr[i]<0)
        count++;
    }
    int maxx=arr[0];
    if(count==n){
        for(int i=0;i<n;i++){
            if(arr[i]>maxx)
            maxx=arr[i];
        }
    }
    if(count==n)
    cout<<maxx<<endl;
    else
    cout<<maxsum<<endl;
}

int main() {
	int t;
	cin>>t;
	while(t--){
	    int arr[100000];
	    int n,count=0;
	    cin>>n;
	    for(int i=0;i<n;i++)
	    cin>>arr[i];
	    max_sum(arr,n,count);
	}
	return 0;
}
